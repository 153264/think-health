{
  "name": "153264/think-health",
  "type": "module",
  "description": "Health check for ThinkPHP",
  "keywords": [
    "thinkphp",
    "health"
  ],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/153264/think-health/issues",
    "source": "https://github.com/153264/think-health",
    "docs": "https://153264.github.io/think-health/"
  },
  "archive": {
    "exclude": [
      "*",
      "!/src/**",
      "!/composer.json",
      "!/README.md",
      "/.gitignore",
      "/.cache",
      "/.php-cs-fixer.dist.php"
    ]
  },
  "autoload": {
    "psr-4": {
      "think\\health\\": "src/"
    }
  },
  "authors": [
    {
      "name": "lpf",
      "email": "2563526478@qq.com"
    }
  ],
  "require": {
    "php": ">=8.2.0",
    "topthink/framework": "^6.1.5|^8.0"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "think": {
      "config": {
        "health": "src/config.php"
      },
      "services": [
        "think\\health\\Service"
      ]
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.67",
    "phpstan/phpstan": "^2.1"
  }
}
